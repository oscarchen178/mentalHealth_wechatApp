/* pages/list/list.wxss */

.gotoButtons button{
  width: 60vw;
  padding: 20px;
  margin: 10px;
  text-align: center;
  border-radius: 20px;
}
.gotoButtons button:nth-child(1){
  background-color:aqua;
}
.gotoButtons button:nth-child(2){
  background-color:skyblue
}
.gotoButtons button:nth-child(3){
  background-color:coral
}
.gotoButtons{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.text_main view{
  height: 60px;
  line-height: 100px;
  text-align:center; 
  font-size: large;
  font-family:'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif 
}

